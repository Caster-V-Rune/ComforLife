<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.SalepicEntity" table="salepic" schema="" catalog="comforlife">
        <id name="picId">
            <column name="picID" sql-type="int" not-null="true"/>
        </id>
        <property name="homeId">
            <column name="homeID" sql-type="int" not-null="true"/>
        </property>
        <property name="pic">
            <column name="pic" sql-type="blob" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>