        </property>
        <property name="busiArea">
            <column name="busiArea" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="room">
            <column name="room" sql-type="int" not-null="true"/>
        </property>
        <property name="hall">
            <column name="hall" sql-type="int" not-null="true"/>
        </property>
        <property name="toilet">
            <column name="toilet" sql-type="int" not-null="true"/>
        </property>
        <property name="area">
            <column name="area" sql-type="int" not-null="true"/>
        </property>
        <property name="fitment">
            <column name="fitment" sql-type="int" not-null="true"/>
        </property>
        <property name="direction">
            <column name="direction" sql-type="int"/>
        </property>
        <property name="cert">
            <column name="cert" sql-type="blob"/>
        </property>
        <property name="floor">
            <column name="floor" sql-type="int"/>
        </property>
        <property name="floorNum">
            <column name="floorNum" sql-type="int"/>
        </property>
        <property name="buildNum">
            <column name="buildNum" sql-type="int"/>
        </property>
        <property name="unitNum">
            <column name="unitNum" sql-type="int"/>
