        <property name="title">
            <column name="title" sql-type="varchar" length="100"/>
        </property>
        <property name="descri">
            <column name="descri" sql-type="text" length="65535"/>
        </property>
        <property name="checkInTime">
            <column name="checkInTime" sql-type="date"/>
        </property>
        <property name="check">
            <column name="check" sql-type="int"/>
        </property>
        <property name="attention">
            <column name="attention" sql-type="int"/>
        </property>
        <property name="latlng">
            <column name="latlng" sql-type="varchar" length="100"/>
        </property>
    </class>
</hibernate-mapping>