        <id name="homeId">
            <column name="homeID" sql-type="int" not-null="true"/>
        </id>
        <property name="userId">
            <column name="userID" sql-type="int" not-null="true"/>
        </property>
        <property name="homeType">
            <column name="homeType" sql-type="int" not-null="true"/>
        </property>
        <property name="rentType">
            <column name="rentType" sql-type="int" not-null="true"/>
        </property>
        <property name="conName">
            <column name="conName" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="homeArea">
